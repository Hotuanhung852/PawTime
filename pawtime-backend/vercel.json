{
    "version": 2,
    "builds": [
      {
        "src": "pawtime-backend/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "PawTime/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "pawtime-backend/server.js"
      }
    ]
  }